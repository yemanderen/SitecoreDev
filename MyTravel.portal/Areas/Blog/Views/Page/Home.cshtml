@model MyTravel.portal.Areas.Blog.Models.PageModel

<h1>Home Page</h1>

<div>
    <h4>@Model.PageHeader</h4>
    <div>
        @if (Model.ContentImage != null)
        {
            <p><img src="@Model.ContentImage.Src" /></p>
        }
        <p>@Model.Body</p>
        <p>@Model.Date</p>
    </div>
</div>
